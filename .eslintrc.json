{
    "env": {
        "browser": true,
        "commonjs": true,
        "es2021": true,
        "mocha":true
    },
    "extends": ["airbnb-base"],
    "parserOptions": {
        "ecmaVersion": 12
    },
    "rules": {
        "no-unused-expressions": "off",
        "no-shadow":"off",
        "no-unused-vars":"warn",
        "import/no-dynamic-require":"warn",
        "global-require":"warn",
        "sequelize/create-indexes-concurrently":"error"
    },
    "plugins":[
        "sequelize",
        "node"
    ]
}
