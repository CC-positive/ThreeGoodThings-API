{
  "name": "threegoodthings-api",
  "version": "0.0.0",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "test-lint": "eslint ./test/*.js --fix",
    "lint": "eslint '**/*.js' --fix",
    "start": "node ./bin/www",
    "sequelize": "sequelize",
    "seed": "yarn sequelize db:seed:undo:all && yarn sequelize db:seed:all",
    "dev-test": "mocha && eslint **/*.js",
    "test": "mocha && eslint '**/*.js'",
    "migrate": "yarn sequelize db:migrate:undo:all && yarn sequelize db:migrate",
    "setup": "yarn migrate && yarn seed",
    "production": "export NODE_ENV=production && node ./bin/www"
  },
  "lint-staged": {
    "**/*.js": [
      "eslint --fix"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "dependencies": {
    "cookie-parser": "~1.4.4",
    "debug": "~2.6.9",
    "dotenv": "^8.2.0",
    "ejs": "~2.6.1",
    "express": "~4.16.1",
    "http-errors": "~1.6.3",
    "morgan": "~1.9.1",
    "node-fetch": "^2.6.1",
    "pg": "^8.5.1",
    "pg-hstore": "^2.3.3",
    "sequelize": "^6.3.5",
    "uuid": "^8.3.1"
  },
  "devDependencies": {
    "chai": "^4.2.0",
    "chai-http": "^4.3.0",
    "eslint": "^7.13.0",
    "eslint-config-airbnb-base": "^14.2.1",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-sequelize": "^1.0.4",
    "husky": "^4.3.0",
    "lint-staged": "^10.5.2",
    "mocha": "^8.2.1",
    "nodemon": "^2.0.6",
    "prettier": "^2.2.0",
    "pretty-quick": "^3.1.0",
    "sequelize-cli": "^6.2.0"
  }
}
